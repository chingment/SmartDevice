<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lumos.smartdevice">

    <application
        android:name=".own.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.sm.SmHomeActivity"></activity>
        <activity android:name=".activity.scenelocker.LockerMainActivity" />
        <activity android:name=".activity.sm.SmAppSceneSettingActivity" />
        <activity android:name=".activity.sm.SmFactorySettingActivity" />
        <activity android:name=".activity.sm.SmLoginActivity" />
        <activity android:name=".activity.sm.SmHelpToolActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.InitDataActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>