<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/widget_common_radio_ic_check" android:state_checked="true" /><!--设置选中图片-->
    <item android:drawable="@drawable/widget_common_radio_ic_uncheck" android:state_checked="false" /><!--设置未选中图片-->
    <item android:drawable="@drawable/widget_common_radio_ic_uncheck" android:state_checked="true" android:state_pressed="true" /><!--设置选中图片-->
    <item android:drawable="@drawable/widget_common_radio_ic_check" android:state_checked="false" android:state_pressed="true" /><!--设置未选中图片-->
</selector>